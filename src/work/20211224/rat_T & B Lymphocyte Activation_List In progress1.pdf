                data: {
                    uniqid,
                    login,
                    pwd
                },
                success: function(data) {

                    var element = document.getElementById("message");
                    element.innerHTML = data;
                },
                error: function() {}
            });
        }

        function logout() {
            const uniqid = document.getElementById('ANGXP').value;
            $.ajax({
                url: `https://www.anygenes.com/logout.php`,
                type: 'POST',
                data: {
                    uniqid
                },
                success: function(data) {
                    window.location.href = data;
                },
                error: function() {}
            });
